<% if (message) { %>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
      Swal.fire({
        title: 'WanderScript',
        text: "<%= message %>",
        icon: "<%= messageType%>",
        timer: 3000,
        showConfirmButton: false
      });
    </script>
  <% } %>